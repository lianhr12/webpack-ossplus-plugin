{
  "name": "webpack-ossplus-plugin",
  "version": "1.1.0",
  "description": "webpack OSS上传插件, 可在 webpack 打包结束后将来 webpack 生成的文件自动上传到 OSS 中，目前支持阿里云、腾讯云、七牛云服务提供商",
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "rimraf dist && rollup -c -w",
    "build": "rimraf dist && rollup -c",
    "changelog": "standard-version",
    "docs": "rimraf docs && typedoc --tsconfig tsconfig.json --options typedoc.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lianhr12/webpack-ossplus-plugin.git"
  },
  "keywords": [
    "webpack",
    "webpack plugin",
    "alioss",
    "cos"
  ],
  "bugs": {
    "url": "https://github.com/lianhr12/webpack-ossplus-plugin/issues"
  },
  "homepage": "https://github.com/lianhr12/webpack-ossplus-plugin#readme",
  "author": "Rope(lhr9311@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.0",
    "standard-version": "^9.5.0",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/webpack": "^5.28.0",
    "ali-oss": "^6.17.1",
    "chalk": "^4.1.2",
    "cos-nodejs-sdk-v5": "^2.11.15",
    "lodash": "^4.17.21"
  }
}
